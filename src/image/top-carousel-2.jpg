    inputs.forEach((input) => {

      if (input.value !== '') {

        if (input.parentElement.classList.contains('mktoFieldWrap')) {

          input.parentElement.classList.add('focus')
          input.classList.add('valid-input')

        }

      }

    })

  })

</script>
