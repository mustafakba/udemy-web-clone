<script type="text/javascript">
	window.isOneTrustActive = true;
	let __attachOneTrustCustomHandlingOnlyOnce = (function() {
		let hasExecuted = false;
		return function() {
			if (hasExecuted === true) {
				return false;
			}
			try {
				let manageCookiePreferencesEl = document.getElementById('cookie-preferences');
				let geo = window.OneTrust.getGeolocationData();
				let footerTextOneTrust_CA = 'Do not sell my personal information';
				let footerTestOneTrust_Other = 'Cookie settings';
				// console.log(geo);
				// If we are in California, change text to "Do not sell my personal information"
				if (geo.country === 'US' && geo.state === 'CA') {
					manageCookiePreferencesEl.innerText = footerTextOneTrust_CA;
				} else {
					manageCookiePreferencesEl.innerText = footerTestOneTrust_Other;
				}
				// Remove "style" attribute from hidden "Manage My Cookie Preferences" link which will reveal it.
				manageCookiePreferencesEl.parentElement.removeAttribute('style');
				// Add click handler
				manageCookiePreferencesEl.addEventListener('click', window.OneTrust.ToggleInfoDisplay);
				// Add consent change handler
				window.OneTrust.OnConsentChanged(function(consentEvent){
					window.__onConsentChanged = window.__onConsentChanged || [];
					window.__onConsentChanged.push(consentEvent);
				});
				// Mark executed to prevent duplicate execution
				hasExecuted = true;
			} catch (e) {
				// Silently swallow error.  Not all pages are going to have footer links.
			}
		}
	})();
	function OptanonWrapper() {
		__attachOneTrustCustomHandlingOnlyOnce();
	}
</script>


